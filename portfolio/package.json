{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:scss": "node-sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
    "compress:css": "node-sass css/style.css css/style.css --output-style compressed",
    "build": "npm-run-all compile:scss prefix:css compress:css"
  },
  "author": "vinay",
  "license": "",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "rimraf": "^5.0.1"
  }
}
